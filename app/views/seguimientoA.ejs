<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="/resources/css/menu-instructor.css">
  <%- include('./partials/boostrap.ejs') %>
  <title>PRESTAMOS | Bandeja de entrada</title>
  <!-- Google Font: Source Sans Pro -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="/resources/css/fontawesome-free/css/all.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="/resources/css/icheck-bootstrap/icheck-bootstrap.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="/resources/css/adminlte.min.css">

  <link rel="stylesheet" href="/resources/css/menu-principal-instructor.css">
  <%- include('./partials/boostrap.ejs') %>
  <title>Menu Prestamos SENA</title>
</head>
<body class="hold-transition sidebar-mini">

  <!--Contenedor de logos-->
  <%- include('./partials/iconos.ejs') %>

  <!--Contenedor de la imagen-->
  <div class="container">
    <img src="/resources/img/logoSena.png" width="200px" class="mx-auto d-block">
  </div>

  <!-- contenedor del menu -->
  <nav>
    <ul>
      <li><a href="menu-aprendiz">Inicio</a></li>
      <li>
        <a href="controlcom">Control de PC</a>
      </li>
      <li>
        <a href="prestamoPC2">Pr√©stamo de PC</a>
      </li>
      <li>
        <a href="seguimientoA">Seguimiento</a>
      </li>
    </ul>
  </nav>

  <div class="wrapper">
    <!-- Navbar -->
    <div class="row justify-content-center mt-9"> <!-- Centrar el div row -->
      <div class="col-md-9">
        <div class="card card-success card-outline">
          <div class="card-header">
            <h3 class="card-title">PRESTAMOS</h3>
            <div class="card-tools">
              <div class="input-group input-group-sm">
                <input type="text" class="form-control" placeholder="Buscar correo">
                <div class="input-group-append">
                  <button class="btn btn-success" type="button">
                    <i class="fas fa-search"></i>
                  </button>
                </div>
              </div>
            </div>
          </div>
          <div class="card-body p-0">
            <div class="mailbox-controls">
              <button type="button" class="btn btn-default btn-sm checkbox-toggle">
                <i class="far fa-square"></i>
              </button>
              <div class="btn-group">
                <button type="button" class="btn btn-default btn-sm"></button>
                <i class="far fa-trash-alt"></i>
                </button>
              </div>
            </div>
            <div class="table-responsive mailbox-messages">
              <table class="table table-hover table-striped">
                <thead>
                  <tr class="bg-success">
                    <th>Id</th>
                    <th>Nombre</th>
                    <th>Tipo</th>
                    <th>Caracteristicas</th>
                    <th>Cantidad</th>
                    <th>Jornada</th>
                    <th>Fecha_res</th>
                    <th>Hora_res</th>
                    <th>Tiempo_requerido</th>
                    <th>Estado_aprobacion</th>
                    <th>Id_usuario</th>
                    <th>Observacion</th>
                  </tr>
                </thead>
                <tbody>
                  <tr>
                    <% datosReserva.forEach((elemento)=>{ %>
                      <tr data-id= <%= elemento.ID_RESERVA  %>>
                        <td><%= elemento.ID_RESERVA  %></td>
                        <td><%= elemento.NOMBRE_INSUMO %></td>
                        <td><%= elemento.TIPO_INSUMO %></td>
                        <td><%= elemento.CARACTERISTICAS %></td>
                        <td><%= elemento.CANTIDAD %></td>
                        <td><%= elemento.JORNADA %></td>
                        <td><%= elemento.FECHA_RES %></td>
                        <td><%= elemento.HORA_RES %></td>
                        <td><%= elemento.TIEMPO_REQUERIDO %></td>
                        <td><%= elemento.ESTADO_APROBACION %></td>
                        <td><%= elemento.ID_USUARIO %></td>
                        <td class="mailbox-name">
                          <a href="comprobante.ejs">El insumo ..... fue rechazado</a>
                        </td>
                      </tr>
                    <% }); %>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>
</html>
