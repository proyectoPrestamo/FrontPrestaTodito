<!DOCTYPE html>
<html lang="es">
<!--prueba-->
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="shortcut icon" href="resources/imagenes/logoSena.png">
  <link rel="stylesheet" href="/resources/css/prestamo-material.css">
  <%- include('./partials/boostrap.ejs') %>
  <title>Prestamo herramientas SENA</title>
</head>



<body>
  <!--Contenedor de logos-->
  <div class="container-logos">
    <div class="user">
      <i class="fa-solid fa-bell fa-3x"></i>
      <i class="fa-solid fa-user-gear fa-3x"></i>
    </div>
  </div>
  <!--Contenedor de la imagen-->
  <div class="container">
    <img src="/resources/img/logoSena.png" width="200px" class="mx-auto d-block">
  </div>
  <!-- contenedor del menu -->
  <div class="container-menu">
    <nav class="navbar navbar-expand mt-8 text-aling-center">
      <div class="collapse navbar-collapse" id="navbarNavDropdown">
        <!-- contenedor de los elementos del menu  -->
        <div class="container-items">
          <div class="inicio">
            <li class="nav-init active">
              <a class="nav-link text-dark" href="#">INICIO<span class="sr-only">(current)</span></a>
            </li>
          </div>
          <div class="control">
            <li class="nav-item">
              <a class="nav-link text-dark" href="#">CONTROL DE AULAS</a>
            </li>
          </div>
          <div class="prestamo">
            <li class="nav-item dropdown">
              <a class="nav-link dropdown-toggle  text-dark" href="#" role="button" data-toggle="dropdown"
                aria-expanded="false">
                PRESTAMOS
              </a>
              <div class="dropdown-menu">
                <a class="dropdown-item" href="#">AMBIENTES</a>
                <a class="dropdown-item" href="#">HERRAMIENTAS</a>
                <a class="dropdown-item" href="#">MATERIALES</a>
              </div>
            </li>
          </div>
          </ul>
        </div>
      </div>
    </nav>
  </div>
  <!-- CONTENEDOR DEL FONDO Y EL LETRERO -->
  <div class="fondo rounded-top">
    <div class="container-letrero rounded-top">
      PRESTAMO
    </div>
    <!-- CONTENEDOR DEL FORMULARIO -->
    <div class="formulario">
      <form class="forma-formulario">
        <h1 class="titulo">Prestamo de materiales</h1>

        <div class="contenedor-items-formulario">
          <input type="text" class="items" placeholder="a">
          <label for="" class="label">Tipo de material</label>
        </div>

        <div class="contenedor-items-formulario">
          <input type="text" class="items" placeholder="a">
          <label for="" class="label">Unidad de medida</label>
        </div>

        <div class="contenedor-items-formulario">
          <input type="text" class="items" placeholder="a">
          <label for="" class="label">Cantidad</label>
        </div>

        <div class="contenedor-items-formulario">
          <input type="text" class="items" placeholder="a">
          <label for="" class="label">Programa de formacion</label>
        </div>

        <div class="contenedor-items-formulario">
          <input type="text" class="items" placeholder="a">
          <label for="" class="label">Numero de ficha</label>
        </div>
        <input type="submit" class="reserva-btn" value="RESERVAR">
        <input type="submit" class="cancelar-btn" value="CANCELAR">
      </form>
      <!-- CONTENEDOR CARRUSEL -->
      <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
          <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
          <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        </ol>
        <div class="fondo-img">
          <div class="carousel-inner">
              <div class="carousel-item active">
                <img src="/resources/img/th.jpg" class="w-70" alt="...">
              </div>
              <div class="carousel-item  ">
                <img src="/resources/img/th.jpg" class="w-70" alt="...">
              </div>
              <div class="carousel-item ">
                <img src="/resources/img/th.jpg" class="w-70" alt="...">
              </div>
          </div>
        </div>
        <button class="carousel-control-prev" type="button" data-target="#carouselExampleIndicators" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
          <span class="sr-only">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-target="#carouselExampleIndicators" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
          <span class="sr-only">Next</span>
        </button>
      </div>

      <!-- CONTENEDOR DEL BOTON DE DEVOLUCIONES -->
      <div class="boton-devolucion">
        <input type="submit" class="reserva-btn" value="DEVOLVER COMO PRODUCTO">
      </div>
    </div>
  </div>
</body>
</html>